{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ListObjectInBucket",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket"
      ],
      "Resource": "arn:aws:s3:::socialMediaBucket"
    },
    {
      "Sid": "ObjectActions",
      "Effect": "Allow",
      "Action": [
        "s3:*Object"
      ],
      "Resource": "arn:aws:s3:::socialMediaBucket/*"
    }
  ]
}